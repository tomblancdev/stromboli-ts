{
  "openapi": "3.0.0",
  "info": {
    "contact": {
      "name": "API Support",
      "url": "https://stromboli/issues"
    },
    "description": "Claude Code container orchestration API - secure, isolated AI execution",
    "license": {
      "name": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    },
    "termsOfService": "https://github.com/tomblanc/stromboli",
    "title": "Stromboli API",
    "version": "1.0"
  },
  "paths": {
    "/auth/logout": {
      "post": {
        "description": "Invalidates a JWT token by adding it to the blacklist",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.LogoutResponse"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.ErrorResponse"
                }
              }
            }
          },
          "503": {
            "description": "Service Unavailable",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.ErrorResponse"
                }
              }
            }
          }
        },
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "summary": "Logout (invalidate token)",
        "tags": [
          "auth"
        ]
      }
    },
    "/auth/refresh": {
      "post": {
        "description": "Generate a new access token using a valid refresh token",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/internal_api.RefreshRequest"
              }
            }
          },
          "description": "Refresh request",
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.TokenResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.ErrorResponse"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.ErrorResponse"
                }
              }
            }
          }
        },
        "summary": "Refresh access token",
        "tags": [
          "auth"
        ]
      }
    },
    "/auth/token": {
      "post": {
        "description": "Generate new JWT access and refresh tokens using API credentials",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/internal_api.TokenRequest"
              }
            }
          },
          "description": "Token request",
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.TokenResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.ErrorResponse"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.ErrorResponse"
                }
              }
            }
          }
        },
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "summary": "Generate JWT tokens",
        "tags": [
          "auth"
        ]
      }
    },
    "/auth/validate": {
      "get": {
        "description": "Validate a JWT token and return its claims",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.ValidateResponse"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.ErrorResponse"
                }
              }
            }
          }
        },
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "summary": "Validate JWT token",
        "tags": [
          "auth"
        ]
      }
    },
    "/claude/status": {
      "get": {
        "description": "Checks if Claude credentials are configured",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.ClaudeStatusResponse"
                }
              }
            }
          }
        },
        "summary": "Claude status",
        "tags": [
          "system"
        ]
      }
    },
    "/health": {
      "get": {
        "description": "Returns the health status of the API with component checks",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.HealthResponse"
                }
              }
            }
          }
        },
        "summary": "Health check",
        "tags": [
          "system"
        ]
      }
    },
    "/jobs": {
      "get": {
        "description": "Returns all async jobs",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.JobListResponse"
                }
              }
            }
          }
        },
        "summary": "List jobs",
        "tags": [
          "jobs"
        ]
      }
    },
    "/jobs/{id}": {
      "delete": {
        "description": "Cancels a pending or running job",
        "parameters": [
          {
            "description": "Job ID",
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "additionalProperties": true,
                  "type": "object"
                }
              }
            }
          },
          "404": {
            "description": "Job not found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.RunResponse"
                }
              }
            }
          },
          "409": {
            "description": "Job cannot be cancelled",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.RunResponse"
                }
              }
            }
          }
        },
        "summary": "Cancel job",
        "tags": [
          "jobs"
        ]
      },
      "get": {
        "description": "Returns the status and result of an async job",
        "parameters": [
          {
            "description": "Job ID",
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.JobResponse"
                }
              }
            }
          },
          "404": {
            "description": "Job not found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.RunResponse"
                }
              }
            }
          }
        },
        "summary": "Get job status",
        "tags": [
          "jobs"
        ]
      }
    },
    "/run": {
      "post": {
        "description": "Executes Claude Code in an isolated Podman container",
        "requestBody": {
          "$ref": "#/components/requestBodies/internal_api.RunRequest"
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.RunResponse"
                }
              }
            }
          },
          "400": {
            "description": "Invalid request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.RunResponse"
                }
              }
            }
          },
          "500": {
            "description": "Execution failed",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.RunResponse"
                }
              }
            }
          },
          "503": {
            "description": "Claude not configured",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.RunResponse"
                }
              }
            }
          }
        },
        "summary": "Run Claude",
        "tags": [
          "execution"
        ]
      }
    },
    "/run/async": {
      "post": {
        "description": "Starts Claude Code execution asynchronously and returns a job ID",
        "requestBody": {
          "$ref": "#/components/requestBodies/internal_api.RunRequest"
        },
        "responses": {
          "202": {
            "description": "Accepted",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.AsyncRunResponse"
                }
              }
            }
          },
          "400": {
            "description": "Invalid request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.RunResponse"
                }
              }
            }
          },
          "503": {
            "description": "Claude not configured",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.RunResponse"
                }
              }
            }
          }
        },
        "summary": "Run Claude async",
        "tags": [
          "execution"
        ]
      }
    },
    "/run/stream": {
      "get": {
        "description": "Executes Claude and streams output in real-time using SSE",
        "parameters": [
          {
            "description": "The prompt to send to Claude",
            "in": "query",
            "name": "prompt",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "description": "Working directory inside container",
            "in": "query",
            "name": "workdir",
            "schema": {
              "type": "string"
            }
          },
          {
            "description": "Session ID for conversation continuation",
            "in": "query",
            "name": "session_id",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Event stream of output lines",
            "content": {
              "text/event-stream": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "400": {
            "description": "Invalid request",
            "content": {
              "text/event-stream": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "503": {
            "description": "Claude not configured",
            "content": {
              "text/event-stream": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        },
        "summary": "Stream Claude output",
        "tags": [
          "execution"
        ]
      }
    },
    "/secrets": {
      "get": {
        "description": "Returns all available Podman secrets that can be injected into agents",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.SecretsListResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.SecretsListResponse"
                }
              }
            }
          }
        },
        "summary": "List secrets",
        "tags": [
          "secrets"
        ]
      }
    },
    "/sessions": {
      "get": {
        "description": "Returns all existing session IDs",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.SessionListResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.SessionListResponse"
                }
              }
            }
          }
        },
        "summary": "List sessions",
        "tags": [
          "sessions"
        ]
      }
    },
    "/sessions/{id}": {
      "delete": {
        "description": "Removes a session and all its stored data",
        "parameters": [
          {
            "description": "Session ID",
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.SessionDestroyResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.SessionDestroyResponse"
                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.SessionDestroyResponse"
                }
              }
            }
          }
        },
        "summary": "Destroy session",
        "tags": [
          "sessions"
        ]
      }
    },
    "/sessions/{id}/messages": {
      "get": {
        "description": "Returns paginated conversation history for a session including all messages, tool calls, and results",
        "parameters": [
          {
            "description": "Session ID (UUID)",
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "description": "Offset for pagination (default: 0)",
            "in": "query",
            "name": "offset",
            "schema": {
              "type": "integer"
            }
          },
          {
            "description": "Number of messages to return (default: 50, max: 200)",
            "in": "query",
            "name": "limit",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.SessionMessagesResponse"
                }
              }
            }
          },
          "400": {
            "description": "Invalid parameters",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.SessionMessagesResponse"
                }
              }
            }
          },
          "404": {
            "description": "Session not found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.SessionMessagesResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.SessionMessagesResponse"
                }
              }
            }
          }
        },
        "summary": "List session messages",
        "tags": [
          "sessions"
        ]
      }
    },
    "/sessions/{id}/messages/{message_id}": {
      "get": {
        "description": "Returns a specific message from session history by UUID, including full content, tool calls, and results",
        "parameters": [
          {
            "description": "Session ID (UUID)",
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "description": "Message UUID",
            "in": "path",
            "name": "message_id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.SessionMessageResponse"
                }
              }
            }
          },
          "400": {
            "description": "Invalid parameters",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.SessionMessageResponse"
                }
              }
            }
          },
          "404": {
            "description": "Message not found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.SessionMessageResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/internal_api.SessionMessageResponse"
                }
              }
            }
          }
        },
        "summary": "Get session message",
        "tags": [
          "sessions"
        ]
      }
    }
  },
  "servers": [
    {
      "url": "//localhost:8080"
    }
  ],
  "components": {
    "requestBodies": {
      "internal_api.RunRequest": {
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/internal_api.RunRequest"
            }
          }
        },
        "description": "Run request",
        "required": true
      }
    },
    "securitySchemes": {
      "BearerAuth": {
        "description": "OAuth2 Bearer token (future)",
        "in": "header",
        "name": "Authorization",
        "type": "apiKey"
      }
    },
    "schemas": {
      "internal_api.AsyncRunResponse": {
        "description": "Response from starting an async Claude execution",
        "properties": {
          "job_id": {
            "example": "job-abc123def456",
            "type": "string"
          }
        },
        "type": "object"
      },
      "internal_api.ClaudeStatusResponse": {
        "description": "Claude configuration status",
        "properties": {
          "configured": {
            "example": true,
            "type": "boolean"
          },
          "message": {
            "example": "Claude is configured",
            "type": "string"
          }
        },
        "type": "object"
      },
      "internal_api.ComponentHealth": {
        "properties": {
          "error": {
            "description": "Error message if status is \"error\"",
            "example": "",
            "type": "string"
          },
          "name": {
            "description": "Name of the component",
            "example": "podman",
            "type": "string"
          },
          "status": {
            "description": "Status is \"ok\" or \"error\"",
            "example": "ok",
            "type": "string"
          }
        },
        "type": "object"
      },
      "internal_api.ErrorResponse": {
        "properties": {
          "error": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "internal_api.HealthResponse": {
        "description": "Health check response",
        "properties": {
          "components": {
            "items": {
              "$ref": "#/components/schemas/internal_api.ComponentHealth"
            },
            "type": "array"
          },
          "name": {
            "example": "stromboli",
            "type": "string"
          },
          "status": {
            "example": "ok",
            "type": "string"
          },
          "version": {
            "example": "0.1.4",
            "type": "string"
          }
        },
        "type": "object"
      },
      "internal_api.JobListResponse": {
        "description": "List of async jobs",
        "properties": {
          "jobs": {
            "items": {
              "$ref": "#/components/schemas/internal_api.JobResponse"
            },
            "type": "array"
          }
        },
        "type": "object"
      },
      "internal_api.JobResponse": {
        "description": "Job status and result",
        "properties": {
          "crash_info": {
            "$ref": "#/components/schemas/stromboli_internal_job.CrashInfo"
          },
          "created_at": {
            "example": "2024-01-15T10:30:00Z",
            "type": "string"
          },
          "error": {
            "type": "string"
          },
          "id": {
            "example": "job-abc123def456",
            "type": "string"
          },
          "output": {
            "example": "Hello!",
            "type": "string"
          },
          "session_id": {
            "example": "sess-abc123def456",
            "type": "string"
          },
          "status": {
            "allOf": [
              {
                "$ref": "#/components/schemas/stromboli_internal_job.Status"
              }
            ],
            "example": "running"
          },
          "updated_at": {
            "example": "2024-01-15T10:31:00Z",
            "type": "string"
          }
        },
        "type": "object"
      },
      "internal_api.LogoutResponse": {
        "properties": {
          "message": {
            "type": "string"
          },
          "success": {
            "type": "boolean"
          }
        },
        "type": "object"
      },
      "internal_api.RefreshRequest": {
        "properties": {
          "refresh_token": {
            "type": "string"
          }
        },
        "required": [
          "refresh_token"
        ],
        "type": "object"
      },
      "internal_api.RunRequest": {
        "description": "Request to execute Claude Code in an isolated container",
        "properties": {
          "claude": {
            "allOf": [
              {
                "$ref": "#/components/schemas/stromboli_internal_types.ClaudeOptions"
              }
            ],
            "description": "Claude configuration - all CLI options exposed"
          },
          "podman": {
            "allOf": [
              {
                "$ref": "#/components/schemas/stromboli_internal_types.PodmanOptions"
              }
            ],
            "description": "Podman configuration"
          },
          "prompt": {
            "description": "Required: the prompt to send to Claude",
            "example": "Analyze this code and suggest improvements",
            "type": "string"
          },
          "webhook_url": {
            "description": "Webhook URL to notify when job completes (async only)",
            "example": "https://example.com/webhook",
            "type": "string"
          },
          "workdir": {
            "description": "Working directory inside the container where Claude will spawn\nUse podman.volumes to mount host paths into the container",
            "example": "/workspace",
            "type": "string"
          }
        },
        "required": [
          "prompt"
        ],
        "type": "object"
      },
      "internal_api.RunResponse": {
        "description": "Response from Claude execution",
        "properties": {
          "error": {
            "description": "Error message (when failed)",
            "example": "",
            "type": "string"
          },
          "id": {
            "description": "Unique run identifier",
            "example": "run-abc123def456",
            "type": "string"
          },
          "output": {
            "description": "Claude's output (when successful)",
            "example": "Here is my analysis...",
            "type": "string"
          },
          "session_id": {
            "description": "Session ID for conversation continuation",
            "example": "sess-abc123def456",
            "type": "string"
          },
          "status": {
            "description": "Execution status: completed, error",
            "example": "completed",
            "type": "string"
          }
        },
        "type": "object"
      },
      "internal_api.SecretsListResponse": {
        "description": "List of available secrets that can be injected into agents",
        "properties": {
          "error": {
            "type": "string"
          },
          "secrets": {
